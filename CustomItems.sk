on join:
	loop all players:
		if loop-player has permission "op":
			set {points.%uuid of loop-player%} to 10000
		else if loop-player has permission "pointsu.have":
			set {points.%uuid of loop-player%} to 1500
		else:
			set {points.%uuid of loop-player%} to 1000
